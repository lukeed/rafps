{
  "name": "rafps",
  "version": "0.0.0",
  "repository": "lukeed/rafps",
  "description": "A tiny (178B) helper for playing, pausing, and setting requestAnimationFrame framerates",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.min.js",
  "main": "dist/index.js",
  "types": "rafps.d.ts",
  "license": "MIT",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "build": "bundt",
    "pretest": "npm run build",
    "test": "tape -r esm test/*.js | tap-spec"
  },
  "keywords": [
    "fps",
    "framerate",
    "cancelAnimationFrame",
    "requestAnimationFrame",
    "raf"
  ],
  "devDependencies": {
    "bundt": "0.4.0",
    "esm": "3.2.25",
    "tap-spec": "5.0.0",
    "tape": "4.13.0"
  }
}
